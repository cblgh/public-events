{
  "name": "events-site",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "get-data": "npm run get-readme",
    "get-meetings": "node -r esm get-meetings.js",
    "get-readme": "node -r esm get-readme.js",
    "build": "bankai build index.js",
    "create": "choo-scaffold",
    "inspect": "bankai inspect index.js",
    "start": "npm run get-data && bankai start index.js",
    "test": "standard && npm run test-deps",
    "test-deps": "dependency-check . && dependency-check . --extra --no-dev -i tachyons",
    "ci": "npm run get-data && npm run build"
  },
  "standard": {
    "ignore": [
      "dist/**"
    ]
  },
  "dependencies": {
    "choo": "^6.13.3",
    "choo-service-worker": "^2.4.0",
    "sheetify": "^7.3.3",
    "tachyons": "^4.11.1"
  },
  "devDependencies": {
    "acorn": "^6.1.1",
    "bankai": "^9.15.0",
    "choo-devtools": "^2.5.1",
    "choo-scaffold": "^1.2.0",
    "dependency-check": "^3.3.0",
    "esm": "^3.2.22",
    "gh-got": "^8.1.0",
    "hasha": "^5.0.0",
    "level": "^5.0.1",
    "smh-markdowner": "^2.1.0",
    "standard": "^12.0.1"
  }
}
